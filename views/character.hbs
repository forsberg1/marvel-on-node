<div class="col-md-8 text-center">
  <div class="spacing transparent">
    <img src="{{marvelThumb character.thumbnail}}" id="character-thub">
    <h1>{{character.name}}</h1>

    <p>{{character.description}}</p>
  </div>
</div>
<div class="col-md-4 border-radius text-center">
  <div class="spacing transparent">
    <h2>Stories</h2>
    {{#each character.stories.items}}
      <p>{{this.name}}</p>
    {{/each}}
    <hr>
    <h2>Comics</h2>
    {{#each character.comics.items}}
      <p>{{this.name}}</p>
    {{/each}}
    <hr>
    <h2>Series</h2>
    {{#each character.series.items}}
      <p>{{this.name}}</p>
    {{/each}}
  </div>
</div>
